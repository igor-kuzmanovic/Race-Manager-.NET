<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>

  <system.serviceModel>
    <client>
      <endpoint address="net.tcp://localhost:8080/LoginService"
                binding="netTcpBinding"
                contract="Service.ILoginService"
                name="LoginService" />
      <endpoint address="net.tcp://localhost:8080/UserService"
                binding="netTcpBinding"
                contract="Service.IUserService"
                name="UserService" />
      <endpoint address="net.tcp://localhost:8080/RaceService"
                binding="netTcpBinding"
                contract="Service.IRaceService"
                name="RaceService" />
      <endpoint address="net.tcp://localhost:8080/DriverService"
                binding="netTcpBinding"
                contract="Service.IDriverService"
                name="DriverService" />
      <endpoint address="net.tcp://localhost:8080/VehicleService"
                binding="netTcpBinding"
                contract="Service.IVehicleService"
                name="VehicleService" />
    </client>
  </system.serviceModel>
  
</configuration>